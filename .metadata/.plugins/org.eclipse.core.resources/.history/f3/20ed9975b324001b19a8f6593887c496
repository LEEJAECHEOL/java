package ch02;


// int, long, double, char, boolean, []배열, String


public class Var06 {

	public static void main(String[] args) {
		// 자동차를 저장 (몇CC, 색깔, 종류)
		int power = 2000;
		String color = "white";
		String name = "소나타";
		String type = "승용차";
		
		int power2 = 2000;
		String color2 = "black";
		String name2 = "BMW";
		String type2 = "승용차";
		
	}
}
