package ch04;

public class PersonApp {

	public static void main(String[] args) {
		Person p1 = new Person();
		p1.name = "최승희";
		p1.job = "의사";
		p1.age = 45;
		p1.gender = '여';
		p1.blood = "A";
		p1.preview();
		Person p2 = new Person();
		p2.name = "이미녀";
		p2.job = "골프 선수";
		p2.age = 28;
		p2.gender = '여';
		p2.blood = "O";
		p2.preview();
		Person p3 = new Person();
		p3.name = "김미남";
		p3.job = "교슈";
		p3.age = 47;
		p3.gender = '남';
		p3.blood = "AB";
		p3.preview();
	}

}
