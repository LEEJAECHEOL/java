package review;

public class Ex03 {

	public static void main(String[] args) {
		// 기본 자료형 = primitive type(원시 자료형)
		// 특징 : 사이즈가 정해져 있음.
		// 결과 : 메모리에 해당 공간을 확보할 수 있음.
		// Call By Value라고 부름 = 해당 메모리 공간에 값이 있음.
		
		boolean t1 = true;
		short t2 = 10;	// 2byte
		char t3 = 'A'; // 2byte
		int t4 = 10;	// 4byte
		long t5 = 20; // 8byte
		
		
		
	}

}
