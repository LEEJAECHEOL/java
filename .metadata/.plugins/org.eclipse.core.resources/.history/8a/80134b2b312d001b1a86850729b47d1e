package review;

interface Remocon {
	void change();
}

class SamsungRemocon implements Remocon {

	@Override
	public void change() {
		System.out.println("삼성채널 변경");
	}
	
}

class LGRemocon implements Remocon {

	@Override
	public void change() {
		System.out.println("LG채널 변경");
	}
	
}

public class Ex12 {

	public static void main(String[] args) {
		Remocon r = new LGRemocon();
		r.change();
		
	}

}
