package ch02;

public class Var04 {

	public static void main(String[] args) {
		// 자리 있는 곳에 
		// 숫자 여러개 1,2,3
		int n1 = 1;
		int n2 = 2;
		int n3 = 3;
		
		// 연속적으로 저장 , 추가가 불가능 
		// 숫자 여러개 1,2,3
		int[] n = {1, 2, 3};
		System.out.println(n[0]);
		System.out.println(n[1]);
		System.out.println(n[2]);
		// 문자 여러개
		char[] c = {'A', 'B', 'C'};
		System.out.println(c[0]);
		System.out.println(c[1]);
		System.out.println(c[2]);
		
	}

}
