package ch04;


class TVs {
	int size;
	String mf;
	public TV() {
		size =32;
		mf ="LG";
		System.out.println(size + "인치"+ mf);
	}
	public TV(String mf) {
		size = 32;
		this.mf = mf;
		System.out.println(size + "인치"+ mf);
	}
	
}
public class TVApp {
	
	public static void main(String[] args) {
		
	}

}
