package ch04;


class Cal {
	
	static void add(int n1, int n2) {
		System.out.println(n1 + n2);
	}
	static int mainus(int n3, int n4) {
		return (n3 - n4);
	}
}

public class Method03 {
	public static void main(String[] args) {
		Cal.add(10, 10);
		int result = Cal.mainus(10, 4);
		System.out.println(result);
	}
}
