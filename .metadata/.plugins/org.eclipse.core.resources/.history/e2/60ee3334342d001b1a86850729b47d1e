package review;

class 저장소 {
	
	String data;
	
	String 다운로드() {
		data = "GTA5";
		try {
			Thread.sleep(3000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		} // 1000 = 1sec
		return data;
	}
}

public class Ex122 {

	public static void main(String[] args) {
		저장소 s = new 저장소();
		String game = s.다운로드();
		System.out.println(game);
	}

}
