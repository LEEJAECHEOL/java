package ch07;

import java.util.ArrayList;

public class Generic02 {

	public static void main(String[] args) {
		// 동시접속이 안됨.
		ArrayList<String> list1 = new ArrayList<>();
		list1.add("Apple");
		list1.add("Banana");
		
		int size = list1.size();
		for (int i = 0; i < size; i++) {
			list1.get(i);
		}
	
	}

}
