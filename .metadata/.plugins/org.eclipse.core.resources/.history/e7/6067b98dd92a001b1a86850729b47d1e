package ch06;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor

// DTO = Data Transfer Object = 통신에서 많이 사용
class UserDto {
	private int id;
	private String name;
}

public class String01 {

	public static void main(String[] args) {
		String data = "id:2,name:홍길동";
	}

}
