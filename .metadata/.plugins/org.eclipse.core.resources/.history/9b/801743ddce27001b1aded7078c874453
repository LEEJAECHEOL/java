package star1;

class Marin {
	// 멤버변수 : 필드 : heep변수 : 전역변수 : 속성 : 프로퍼티
	// field, property 는 private로 만든다.  마법X
	private String name;
	private int hp;
	private int attack;
	
	public Marin(String name, int hp, int attack) {
		// 1. this는 heap변수
		// 2.this 는 new가 된 heap공간을 바인딩 한다
		
		this.name = name;
		this.hp = hp;
		this.attack = attack;
	}
	
	public Marin() {
		// TODO Auto-generated constructor stub
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}

	public int getAttack() {
		return attack;
	}

	public void setAttack(int attack) {
		this.attack = attack;
	}
	
	
}
