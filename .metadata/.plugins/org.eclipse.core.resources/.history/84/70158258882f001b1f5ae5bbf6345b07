package ch13;

public class Thread03 {
	int sum = 0;
	
	class NeewThread implements Runnable {
		@Override
		public void run() {
			for (int i = 0; i < 10; i++) {
				sum = sum + 10;
				try {
					Thread.sleep(200);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
			}
		}
		
	}

	public static void main(String[] args) {

	}

}
