package composit;

import lombok.Data;

@Data	//getter setter

public class BigBurger {
	private int price;
	private String desc;
	public BigBurger() {
		this(1000, "버거");
	}
	public BigBurger(int price, String desc) {
		this.price = price;
		this.desc = desc;
		System.out.println(desc + "가 만들어졌습니다.");
	}
	
}
