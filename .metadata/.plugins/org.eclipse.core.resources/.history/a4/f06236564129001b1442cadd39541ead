package ch03;

import java.util.Scanner;

public class Example16 {

	public static void main(String[] args) {
		String[] str = {"가위", "바위", "보"};
		Scanner s = new Scanner(System.in);
		System.out.println("컴퓨터와 가위 바위 보 게임을 합니다.");
		boolean check = true;
		String myValue;
		while(check) {
			System.out.print("가위 바위 보! >> ");
			myValue = s.nextLine();
			
		}
	}

}
