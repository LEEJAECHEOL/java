
package ch05;

class Animal{

	void sound() {
		System.out.println();
	}
}
class Dog {

	void sound() {
		System.out.println("멍멍");
	}
}
class Cat {
	void sound() {
		System.out.println("냐옹");
	}
}

public class Inherit03 {
	static void start (Dog d) {
		d.sound();
	}
	static void start (Cat c) {
		c.sound();
	}
	public static void mai(String[] args) {
		Dog d = new Dog();
		Inherit03.start(d);;
		
	}

}
